#include <stdio.h>
#include <stdlib.h>

int strLen(char* str)
{
	int count = 0;
	while (1)
	{
		if (str[count] == NULL)
		{
			return count;
		}
		count++;
	}
}

int main(void)
{
	char* ip;
	int i = 0;
	for (i = 0; ip[i] == NULL; i++) 
	{ 
		scanf("%s", ip); 
	}
	ip = (char*)malloc(sizeof(char));

	strLen(ip);
	return 0;
}
